{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements.txt"
      }
    },
    {
      "src": "api/direct_test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements.txt"
      }
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/direct_test.py"
    },
    {
      "src": "/api/auth",
      "dest": "api/auth.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
} 