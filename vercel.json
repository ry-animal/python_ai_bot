{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/direct_test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements-direct.txt"
      }
    },
    {
      "src": "api/openai-test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements-openai.txt"
      }
    },
    {
      "src": "api/langchain-test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements-langchain.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test(.*)",
      "dest": "api/direct_test.py"
    },
    {
      "src": "/api/openai-test(.*)",
      "dest": "api/openai-test.py"
    },
    {
      "src": "/api/langchain-test(.*)",
      "dest": "api/langchain-test.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
} 